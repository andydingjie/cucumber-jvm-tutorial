<web-app>
    <servlet>
        <servlet-name>BacklogServlet</servlet-name>
        <servlet-class>ngpanwei.backlog.app.backlog.BacklogServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TestEnvServlet</servlet-name>
        <servlet-class>ngpanwei.bdd.tests.TestEnvServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>BacklogServlet</servlet-name>
        <url-pattern>/task/*</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TestEnvServlet</servlet-name>
        <url-pattern>/env/*</url-pattern>
    </servlet-mapping>
</web-app>